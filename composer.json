{
    "name": "preventhivok/phok_platform_profile",
    "description": "Drupal 8 installation profile for PHOK Platform.",
    "type": "drupal-profile",
    "keywords": [
        "Drupal",
        "PHOK Platform",
        "profile",
        "Prevent HIV Oklahoma"
    ],
    "license": "GPL-2.0+",
    "homepage": "https://github.com/PreventHIVOk/drupal-phok_platform_profile#readme",
    "support": {
        "issues": "https://github.com/PreventHIVOk/drupal-phok_platform_profile/issues",
        "source": "https://github.com/PreventHIVOK/drupal-phok_platform_profile.git"
    },
    "authors": [
        {
            "name": "Curtis Blumer",
            "email": "curtis.blumer@gmail.com",
            "homepage": "http://curtapedia.com",
            "role": "Developer"
        }
    ],
    "replace": {
        "drupal/phok_platform_profile": "*"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "require": {
        "php": "^7.0",
        "drupal/core": "^8.3",
        "drupal/admin_toolbar": "^1.19",
        "drupal/dropzonejs": "^1.0-alpha6",
        "drupal/entity_browser": "^1.0-rc2",
        "drupal/media_entity": "^1.6",
        "drupal/pathauto": "^1.0-rc1",
        "preventhivok/content_tags": "~1.0"
    },
    "scripts": {
        "test": "echo Error: No tests have been specified && exit 1"
    },
    "autoload": {
        "psr-4": {
            "Drupal\\phok_platform_profile\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Drupal\\phok_platform_profile\\Tests\\": "tests"
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}